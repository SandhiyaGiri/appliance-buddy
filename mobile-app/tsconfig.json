{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "types": ["expo", "react", "node"],
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "./*.ts",
    "./*.tsx",
    "./src/**/*.ts",
    "./src/**/*.tsx"
  ]
}
